    "suggestion_timeout": 2
  },
  "completions": {
    "file_completion": true,
    "command_completion": true,
    "argument_completion": true,
    "smart_completion": true
  },
  "history": {
    "max_history_size": 1000,
    "persistent_history": true,
    "history_analysis": true
  }
}